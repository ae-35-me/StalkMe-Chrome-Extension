{
  "manifest_version": 3,
  "name": "StalkMe",
  "version": "1.0",
  "description": "A demonstration of excessive permissions risks.",
  "permissions": [
    "webRequest",
    "webRequestBlocking",
    "tabs",
    "scripting",
    "cookies",
    "storage"
  ],
  "host_permissions": [
    "https://*/*",
    "http://*/*",
    "file:///*"
  ],
  "action": {
    "default_popup": "popup.html",
    "default_icon": {
      "16": "images/icon16.png",
      "48": "images/icon48.png",
      "128": "images/icon128.png"
    }
  },
  "background": {
    "service_worker": "background.js"
  },
  "icons": {
    "16": "images/icon16.png",
    "48": "images/icon48.png",
    "128": "images/icon128.png"
  }
}